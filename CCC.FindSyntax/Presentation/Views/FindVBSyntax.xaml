        <dxlc:LayoutGroup Header="Find ImportsStatement Syntax" 
        	View="GroupBox"
            Orientation="Vertical"
            IsCollapsible="True" IsCollapsed="True">

            <dxlc:LayoutGroup
            	Orientation="Horizontal">

                <dxlc:LayoutGroup
                	Orientation="Vertical">
                    <dxlc:LayoutItem>
                        <dxe:CheckEdit Content="Use Regular Expressions"
                                       IsChecked="{Binding ImportsStatementUseRegEx}"/>
                    </dxlc:LayoutItem>

                    <dxlc:LayoutItem Label="Imports">
                        <dxe:TextEdit Text="{Binding ImportsStatementRegEx}"                                      
                                      IsEnabled="{cb:Binding 
                                            ElementName=ceImportsStatementUseRegEx, 
                                            Path=IsChecked, Mode=OneWay}"
                                      ToolTip="Supports Regular Expressions"/>
                    </dxlc:LayoutItem>

                </dxlc:LayoutGroup>

            </dxlc:LayoutGroup>

            <dxlc:LayoutItem>

                <Button Content="ImportsStatement Walker" 
                        Style="{StaticResource SyntaxQueryButton}"
                        Command="{Binding ImportsStatementWalkerCommand}" 
                        Tag="ImportsStatementWalker" />
            </dxlc:LayoutItem>

        </dxlc:LayoutGroup>

